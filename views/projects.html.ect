<% if @projects1.length: %>
  <style>table, th, td {border: 1px solid black;}</style>
  <table style="width:100%">
    <tr>
    <% for key in Object.keys(@projects1[0]): %>
    <th><%= key %></th>
    <% end %>
    </tr>
<% for project in @projects1: %>
    <tr>
      <% for value in Object.values(project): %>
      <td><%= value %></td>
      <% end %>
      </tr>
<% end %>
  </table>
<% else: %>
No projects
<% end %>
